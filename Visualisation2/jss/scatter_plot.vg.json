{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/YunusNuzili/FIT3179/main/Visualisation2/Dataset/Accidents_VIC2.csv"
    },
    "vconcat": [
      {
        "width": 500,
        "height": 250,
        "mark": "point",
        "encoding": {
          "x": {
            "field": "LONGITUDE",
            "type": "quantitative",
            "axis": {"title": ""},
            "scale": {"domain" : [140,150]}
          },
          "y": {
            "field": "LATITUDE", 
            "type": "quantitative"},
            "scale": {"domain" : [-34, -39]},
          "tooltip" : [
            {"field": "OBJECTID", 
            "type":"quantitative", 
            "title":"Number of Accidents"}
          ]
        }
      }
    ]
  }