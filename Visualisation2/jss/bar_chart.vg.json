{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/YunusNuzili/FIT3179/main/Visualisation2/Dataset/Accidents_VIC2.csv"
    },
    "vconcat": [
      {
        "width": 350,
        "height": 240,
        "mark": "bar",
        "encoding": {
          "x": {
            "field": "DAY_OF_WEEK",
            "type": "nominal",
            "axis": {"title": ""}
          },
          "y": {"aggregate": "count","field": "OBJECTID", "type": "quantitative"},
          "tooltip" : [
            {"aggregate":"count",
            "field": "OBJECTID", 
            "type":"quantitative", 
            "title":"Number of Accidents"}
          ]
        }
      }
    ]
  }